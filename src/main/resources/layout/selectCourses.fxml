<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="me.eddie.blackboardDownloader.gui.controller.SelectCoursesController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
             <ScrollPane fitToHeight="true">
                 <content>
                     <VBox prefHeight="250.0" prefWidth="790.0">
                         <Label text="Select Courses to download: "/>
                         <VBox fx:id="coursesList" prefHeight="250.0" prefWidth="790.0">

                             <padding>
                                 <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                             </padding>
                         </VBox>
                         <padding>
                             <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                         </padding>
                     </VBox>
                 </content>
             </ScrollPane>
         </center>
         <bottom>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="10.0" prefWidth="200.0" BorderPane.alignment="BOTTOM_RIGHT">
               <children>
                  <Button mnemonicParsing="false" text="Next" fx:id="nextButton">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </bottom></BorderPane>
   </children>
</AnchorPane>
