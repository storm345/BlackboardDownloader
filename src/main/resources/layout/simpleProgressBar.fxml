<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="150.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="me.eddie.blackboardDownloader.gui.controller.ProgressBarController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
             <VBox prefHeight="250.0" prefWidth="445.0">
                 <Label fx:id="operationDesc" text="Doing operation...">
                     <VBox.margin>
                         <Insets bottom="10.0" />
                     </VBox.margin></Label>
                 <Label fx:id="operationStatus" text="Status...">
                     <VBox.margin>
                         <Insets bottom="2.0" />
                     </VBox.margin></Label>
               <ProgressBar fx:id="progressBar" prefWidth="430.0" progress="0.0" />
                 <Label fx:id="operationStatus2" text="">
                     <VBox.margin>
                         <Insets bottom="2.0" />
                     </VBox.margin></Label>
                 <padding>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </padding>
             </VBox>
         </center></BorderPane>
   </children>
</AnchorPane>
